{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "A RESTful API for managing a shop, built with Node.js, Express, PostgreSQL, and TypeScript.",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && jasmine",
    "start": "ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "prettier": "prettier --write \"src/**/*.{ts,js,json,md}\""
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "dotenv": "^16.5.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^5.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/pg": "^7.14.7",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jasmine": "^5.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^5.8.3"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dosiichuk/store-backend.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/dosiichuk/store-backend/issues"
  },
  "homepage": "https://github.com/dosiichuk/store-backend#readme"
